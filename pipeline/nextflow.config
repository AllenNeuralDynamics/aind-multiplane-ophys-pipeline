process {
    executor = 'awsbatch'
    queueSize = 100
    maxForks = 16          
    errorStrategy = 'retry'       
    maxRetries = 20  
    maxErrors = 100
    resourceLabels = ['allen-batch-pipeline': 'Multiplane pipeline (v5.0) with file splitting']
}
env {
    ECS_TASK_METADATA_RPS_LIMIT = "5,10"
}